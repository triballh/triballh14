local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local loadedKey = loadstring(game:HttpGet('https://pastebin.com/raw/THbKzz32'))()

print(loadedKey)

getgenv().inputKey = ""

function LoadMain()
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
loadstring(game:HttpGet('https://pastebin.com/raw/SvJEjGZS'))()--script
end

function readKey()
    if readfile("/triballhKey/Key.txt") == loadKey then
Rayfield:Notify({
   Title = "sistema de contraseñas",
   Content = "ejecutando sistema de contraseñas.",
   Duration = 4.0,
   Image = 4483362458,

})
        LoadMain()
        else
        Rayfield:Notify({
   Title = "sistema de contraseñas",
   Content = "ejecutando sistema de contraseñas.",
   Duration = 4.0,
   Image = 4483362458,

})
        local KeyWindow = Rayfield:CreateWindow({
            Name = "Key system",
            LoadingTitle = "Key system",
            LoadingSubtitle = "triballh",
            ConfigurationSaving = {
               Enabled = false,
               FolderName = nil, 
               FileName = "Big Hub"
            },
            Discord = {
               Enabled = false,
               Invite = "ABCD", 
               RememberJoins = true 
            },
            KeySystem = false, 
            KeySettings = {
               Title = "triballh hub",
               Subtitle = "sistema de key",
               Note = "perfil (www.roblox.com/users/365651935/profile)",
               FileName = "SiriusKey",
               SaveKey = true,
               GrabKeyFromSite = false, 
               Key = "123"
            }
         })
         local Tab = KeyWindow:CreateTab("sistema de contraseña", 4483362458)
         local Input = Tab:CreateInput({
   Name = "contraseña:",
   PlaceholderText = "ingrese la contraseña",
   RemoveTextAfterFocusLost = false,
   Callback = function(Text)
   getgenv().inputKey = Text
   end,
})
local Button = Tab:CreateButton({
   Name = "haga click",
   Callback = function()
   if getgenv().inputKey == loadedKey then
        writefile("/triballhKey/Key.txt", loadedKey)
       Rayfield:Notify({
   Title = "triballh hub",
   Content = "ejecutando triballh hub.",
   Duration = 4.0,
   Image = 4483362458,

})
task.wait(3)
       Rayfield:Destroy()
       LoadMain()
       else
       Rayfield:Notify({
   Title = "contraseña incorrecta",
   Content = "intentalo de nuevo.",
   Duration = 4.0,
   Image = 4483362458,
})
   end
   end,
})
        end    
end

if isfolder("/triballhKey") then
    if isfile("/triballhKey/Key.txt", "")then
        readKey()
    else 
        writefile("/triballhKey/Key.txt", "")
        readKey()
    end
 else
    makefolder("triballhKey")
    writefile("/triballhKey/Key.txt", "")
    readKey()
end

    
